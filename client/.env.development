# Development Environment Configuration
# This file contains development-specific environment variables

# API Configuration
VITE_API_URL=http://localhost:5000/api

# App Configuration
VITE_APP_NAME=TechVerse
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_SUPPORT=true
VITE_ENABLE_REVIEWS=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_ENABLE_LOGGING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_MOCK_API=false

# External Services (Development Keys)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-development-key
VITE_GOOGLE_ANALYTICS_ID=
VITE_FACEBOOK_PIXEL_ID=

# Request Configuration
VITE_REQUEST_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3
VITE_RETRY_DELAY=1000
VITE_CACHE_TTL=300000

# Security
VITE_CORS_ENABLED=true
VITE_SECURITY_LEVEL=low

# Build Configuration
VITE_BUILD_HASH=dev-build