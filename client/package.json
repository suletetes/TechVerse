{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate-build && node src/scripts/validateConfig.js && vite build",
    "build:production": "NODE_ENV=production npm run build",
    "build:no-validate": "vite build",
    "preview": "vite preview",
    "validate-config": "node src/scripts/validateConfig.js",
    "validate-build": "node ../scripts/validate-build-config.js",
    "detect-drift": "node ../scripts/detect-config-drift.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:integration": "vitest run src/components/__tests__/integration/",
    "test:performance": "vitest run --reporter=verbose --testTimeout=30000",
    "test:unit": "vitest run src/components/__tests__/ --exclude=**/integration/**",
    "test:hooks": "vitest run src/hooks/__tests__/",
    "test:context": "vitest run src/context/__tests__/",
    "test:components": "vitest run src/components/__tests__/ --exclude=**/integration/**",
    "test:auth": "vitest run src/components/__tests__/Auth/",
    "test:common": "vitest run src/components/__tests__/Common/",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results.xml",
    "test:visual": "npm run build-storybook && chromatic --exit-zero-on-changes",
    "test:storybook": "test-storybook",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6006",
    "test:all": "node src/test/test-runner.js"
  },
  "devDependencies": {
    "@mswjs/data": "^0.16.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^9.1.13",
    "@storybook/react-vite": "^9.1.13",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.0.0",
    "axe-core": "^4.11.0",
    "chromatic": "^13.3.1",
    "jest-image-snapshot": "^6.5.1",
    "jsdom": "^25.0.0",
    "msw": "^2.11.6",
    "msw-storybook-addon": "^2.0.6",
    "vite": "^7.1.2",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/query-persist-client-core": "^5.91.4",
    "@tanstack/query-sync-storage-persister": "^5.90.7",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tiptap/extension-bullet-list": "^3.7.2",
    "@tiptap/extension-color": "^3.7.2",
    "@tiptap/extension-image": "^3.7.2",
    "@tiptap/extension-link": "^3.7.2",
    "@tiptap/extension-list-item": "^3.7.2",
    "@tiptap/extension-ordered-list": "^3.7.2",
    "@tiptap/extension-text-align": "^3.7.2",
    "@tiptap/extension-text-style": "^3.7.2",
    "@tiptap/react": "^3.7.2",
    "@tiptap/starter-kit": "^3.7.2",
    "axios": "^1.12.2",
    "axios-retry": "^4.5.0",
    "bootstrap": "^5.3.2",
    "lucide-react": "^0.400.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
